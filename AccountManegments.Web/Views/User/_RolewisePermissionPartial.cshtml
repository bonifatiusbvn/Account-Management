@model IEnumerable<AccountManagement.DBContext.Models.ViewModels.UserModels.UserRoleModel>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

@foreach (var item in Model)
{

    <tr id="siteCard_@item.RoleId" onclick="EditRoleWiseFormDetails('@item.RoleId',this)">
        <td>@item.Role</td>
        <td>
            <a class="btn text-primary" onclick="EditRoleWiseFormDetails('@item.RoleId',this)" title="Edit" aria-label="Edit"><i class="fadeIn animated bx bx-edit"></i></a>
        </td>
    </tr>
}
@section Scripts {

    <script src="~/moduls/rolewiseformpermissionscript.js"></script>
}